  border-radius: 50%;
  background: -webkit-linear-gradient(left top, #fff, #ccfff7); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(bottom right, #fff, #ccfff7); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(bottom right, #fff, #ccfff7); /* Firefox 3.6 - 15 */
  background: linear-gradient(to bottom right, #fff, #ccfff7); /* 标准的语法（必须放在最后） */
}

.progress_box {
  position: relative;
  width: 120px;
  height: 120px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 20rpx auto;
  z-index:-1;
}

.progress_bg {
  position: absolute;
  width: 120px;
  height: 120px;
  z-index: -1;
}

.progress_canvas {
  width: 120px;
  height: 120px;
  z-index: -1;
}

.progress_text {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
}

.progress_info {
  font-size: 26rpx;
  padding-left: 16rpx;
  letter-spacing: 2rpx;
}

.progress_dot {
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background-color: #fb9126;
}

.question {
  font-size: 46rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(254, 254, 254, 1);
  margin-bottom: 40rpx;
}

.anwser-box {
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 36rpx;
  background: #fff;
  color: #018ce3;
  margin: 40rpx auto;
  text-align: center;
  position: relative;
}
.anwser-box>image{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  right: 20rpx;
  top: 20rpx;
}

.anwser-box-result {
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 36rpx;
  background: #fff;
  color: #96c5e2;
  margin: 40rpx auto;
  text-align: center;
}

.result:nth-of-type(1) {
  background: #2ae972;
  color: #ffffff;
}

.result:nth-of-type(2) {
  background: #ff6f6f;
   color: #ffffff;
}

.result:nth-of-type(3) {
  background: #fff;
  color: #018ce3;
}


/* for custom modal */

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.8;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-content {
  position: fixed;
  top: 26%;
  left: 0;
  z-index: 9500;
  width: 500rpx;
  height: 780rpx;
  overflow: hidden;
  padding: 120rpx 50rpx 50rpx;
  margin: -180rpx 70rpx;
  /* background: #fff; */
  border-radius: 20rpx;
}

.modal-content-box {
  position: fixed;
  top: 14%;
  left: 10%;
  z-index: 9600;
  width: 80%;
  height: 80%;
  overflow: hidden;
  border-radius: 2rpx;
}

