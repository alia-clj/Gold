.jidu {
  position: fixed;
  right: 0;
  top: 100rpx;
  border: 1rpx solid #fefefe;
  width: 150rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 26rpx;
  color: #17a7a9;
  background: -webkit-linear-gradient(left top, #fff, #ccfff7); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(bottom right, #fff, #ccfff7); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(bottom right, #fff, #ccfff7); /* Firefox 3.6 - 15 */
  background: linear-gradient(to bottom right, #fff, #ccfff7); /* 标准的语法（必须放在最后） */
  border-top-left-radius: 36rpx;
  border-bottom-left-radius: 36rpx;
}

.fenshu {
  position: fixed;
  left: 0;
  top: 100rpx;
  border: 1rpx solid #fefefe;
  width: 150rpx;
