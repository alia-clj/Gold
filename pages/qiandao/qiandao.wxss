.fixed-box {
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 1;
}

.testResult {
  position: fixed;
  text-align: center;
  width: 100%;
  height: 38px;
  font-family: PingFang-SC-Bold;
  line-height: 38px;
}

.testResult:nth-of-type(1) {
  top: 80rpx;
  font-size: 56rpx;
  color: #000;
  font-weight: bold;
}

.testResult:nth-of-type(2) {
  top: 160rpx;
  font-size: 36rpx;
  color: #fff;
  width: 60%;
  left: 20%;
  border-radius: 30rpx;
  background-color: #000;
}

.seeReport {
  width: 301rpx;
  height: 71rpx;
  line-height: 70rpx;
  color: #fff;
  background: #6151f1;
  position: fixed;
  bottom: 70rpx;
  left: calc(50% - 150rpx);
  text-align: center;
}

.qiandao-box {
  width: 78%;
  height: 340rpx;
  position: fixed;
  left: 10%;
  bottom: 220rpx;
  z-index: 100;
}

.qiandao-box>li {
  width: 37%;
  height: 120rpx;
  border-radius: 24rpx;
  float: left;
  margin: 20px 6%;
  text-align: center;
  line-height: 110rpx;
  color: #fff;
}

.qiandao-box>li>span {
  font-size: 70rpx;
  font-weight: bold;
}

.active{
  background: #c8c2ff;

}

.actived{
  background:#FF7D35
}


/* for custom modal */

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.8;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-content {
  position: fixed;
  top: 26%;
  left: 0;
  z-index: 9500;
  width: 500rpx;
  height: 780rpx;
  overflow: hidden;
  padding: 120rpx 50rpx 50rpx;
  margin: -180rpx 70rpx;
  /* background: #fff; */
  border-radius: 20rpx;
}

.modal-content-box {
  position: fixed;
  top: 14%;
  left: 10%;
  z-index: 9600;
  width: 80%;
  /* height: 80%; */
  overflow: hidden;
  border-radius: 2rpx;
}

.test-box {
  width: 70%;
  height: 161rpx;
  position: fixed;
  top: 360rpx;
  left: 15%;
  z-index: 96666;
}

.test-box>image {
  width: 100%;
  height: 600rpx;
  position: absolute;
  left: 0%;
  top: -60rpx;
}

.test-box-p {
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
  color: #fff;
  display: block;
  line-height: 100rpx;
  text-align: center;
  position: absolute;
  left: 0;
}

.test-box-p:nth-of-type(1) {
  top: 0;
  font-size: 50rpx;
  font-weight: bold;
  background: -webkit-gradient(linear, left top, left bottom, from(#f8b766), to(#ee7138));
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.test-box-p:nth-of-type(2) {
  top: 60rpx;
  font-size: 42rpx;
}

.test-box-p:nth-of-type(3) {
  top: 120rpx;
  font-size: 32rpx;
}

.test-box-p:nth-of-type(4) {
  top: 160rpx;
  font-size: 32rpx;
}

.btn-box {
  width: 300rpx;
  height: 70rpx;
  background: transparent;
  color: #fff;
  text-align: center;
  line-height: 70rpx;
  position: absolute;
  z-index: 99999999;
  top: 840rpx;
  left: calc(50% - 150rpx);
}

