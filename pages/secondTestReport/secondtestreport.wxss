

.testResult {
  position: fixed;
  text-align: center;
  width: 100%;
  height: 34px;
  font-family: PingFang-SC-Bold;
  color: rgba(255, 255, 255, 1);
  line-height: 38px;
}

.testResult:nth-of-type(1) {
  top: 40rpx;
  font-size: 56rpx;
  color: #ffbc66;
  font-weight: bold;
}

.testResult:nth-of-type(2) {
  top: 140rpx;
  font-size: 36rpx;
  color: #e0d1f7;
}

.testResult:nth-of-type(3) {
  top: 200rpx;
  font-size: 36rpx;
}

.resultImg {
  width: 100%;
  height: 600rpx;
  position: fixed;
  left: 0;
  top: 300rpx;
}

.resultImgUp {
  width: 200rpx;
  height: 200rpx;
  position: fixed;
  right: 5%;
  top: 400rpx;
}

.seeReport{
   width:301rpx;
height:71rpx;
